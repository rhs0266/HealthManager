<Panel ux:Class="healthMgr.Button" Margin="10" Padding="10">
	<string ux:Property="Text"/>

	<Rectangle Layer="Background" Color="Gray" CornerRadius="4">
		<DropShadow Angle="90" Distance="1" Spread="0.2" Size="2" Color="#00000060"/>
	</Rectangle>
	<healthMgr.Text Value="{ReadProperty Text}" FontSize="16" TextAlignment="Center"/>
	<healthMgr.ButtonPress/>
</Panel>